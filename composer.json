{
  "name": "pvtl/laravel-api-authentication",
  "description": "Ahoy! - A package to implement JWT into Laravel",
  "keywords": [
    "laravel",
    "frontend",
    "jwt"
  ],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Pivotal Agency",
      "homepage": "http://pivotalagency.com.au",
      "email": "tech@pivotalagency.com.au"
    }
  ],
  "require": {
    "laravel/framework": "5.5.*",
    "tymon/jwt-auth": "^0.5.12"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "classmap": [],
    "psr-4": {
      "Pvtl\\ApiAuthentication\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [],
    "psr-4": {
      "Pvtl\\ApiAuthentication\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tymon\\JWTAuth\\Providers\\JWTAuthServiceProvider",
        "Pvtl\\ApiAuthentication\\AuthenticationServiceProvider"
      ],
      "aliases": {
        "JWTAuth": "Tymon\\JWTAuth\\Facades\\JWTAuth",
        "JWTFactory": "Tymon\\JWTAuth\\Facades\\JWTFactory"
      }
    }
  },
  "scripts": {
    "test": "phpunit",
    "post-package-install": [
      "php artisan vendor:publish --provider='Tymon\\JWTAuth\\Providers\\JWTAuthServiceProvider'"
    ]
  }
}
